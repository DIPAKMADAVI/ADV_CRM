
<%= form_with model: @MediaAsset, class: "form-horizontal" do |form| %>
<div class="form-row">
<div class="column">
     <div class="form-group col-md-4 mb-3">
          <%= form.label :media_Name  %>
          <%= form.text_field :view_media_type,  disabled: true  , class: "form-control"  %>
     </div>
</div>
<div class="column">
     <div class="form-group col-md-4 mb-3">
     <%= form.label :sub_media %>
     <%= form.text_field :view_sub_media_type, disabled: true , class: "form-control" %>
     </div>
</div>
</div>


<div class="form-row">
     <div class="form-group col-md-4 mb-3">
          <%= form.label :state  %>
          <%= form.text_field :view_state , disabled: true , class: "form-control" %>
     </div>

     <div class="form-group col-md-4 mb-3">
     <%= form.label :district %>
     <%= form.text_field :view_district, disabled: true , class: "form-control" %>
     </div>
</div>


<div class="form-group">
<%= form.label :town_city, class: "control-label col-sm-2" %>
<div class="col-sm-10">
  <%= form.text_field :tview_own_city , disabled: true , class: "form-control" %>
</div>
</div>


<div class="form-group">
<%= form.label :dimentions, class: "control-label col-sm-2" %>
<div class="col-sm-10">
  <%= form.text_field :view_dimentions,  disabled: true , class: "form-control" %>
</div>
</div>
 

<div class="form-group">
<%= form.label :area_location, class: "control-label col-sm-2" %>
<div class="col-sm-10">
  <%= form.text_field :view_area_location,  disabled: true , class: "form-control" %>
</div>
</div>

<div class="form-group">
<%= form.label :number_of_count, class: "control-label col-sm-2" %>
<div class="col-sm-10">
  <%= form.text_field :view_area_lnumber_of_countocation, disabled: true , class: "form-control" %>
</div>
</div>
     


<% end %>


