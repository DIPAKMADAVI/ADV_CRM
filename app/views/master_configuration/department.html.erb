<div class="modal modalleng fade"  id="add_channel" tabindex="-1" role="dialog">
<div class="modal-dialog" role="document">
<div class="modal-content">
<div class="modal-header">
<h5 class="modal-title">Avertisement vvvvvvvvvvvvvvchannell</h5>
<button type="button" class="close" data-dismiss="modal" aria-label="Close">
<span aria-hidden="true">&times;</span>
</button>
</div>

<div class="modal-body">

<%= form_with url: "//...adv_channel", method: :post do |form| %>
<div class: "form-control"> 
<%= form.label :channel_name , "Channel Name" %>
<%= form.text_field :channel_name,  placeholder: "Enter channel name", required: true, name: "channel[channel_name]" %>
</div>

<div class: "form-control">
<%= form.label :sub_channel_name , "Sub Channel Name" %>
<%= form.text_field :sub_channel,   placeholder: "Enter channel name", required: true, name: "channel[sub_channal_name]" %>
</div>

<div class: "form-control">
<%= form.label :desc , "Description" %>
<%= form.text_field :desc,  placeholder: " description", name: "channel[description]"%>
</div>

<div class="modal-footer">
<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
<%= form.submit 'Add Channel', class:'btn btn-secondary' %>
</div>
<% end %> 


</div>

